	<queryString>
		<![CDATA[SELECT
     a.`idDispatch_Order`,
     b.`name`,
     a.`departure_date`,
     a.`arrival_date`,
     case a.`status`
      when 1 then "Atendido"
      when 2 then "Pendiente"
      when 3 then "En Vehiculo"	
      when 4 then "Cancelado"
     end  as status,
     a.`Picking_Order_idPicking_Order`
FROM
     `dispatch_order` a INNER JOIN `client` b ON (a.`idClient` = b.`idClient`)
where a.departure_date>=  $P{fecI}
and a.arrival_date<= $P{fecF}]]>
	</queryString>
